<!DOCTYPE html>
<html lang="en">
<body>
  <ul>
    <% for(var url in urls) {%>
    <li>
      <%= url %>
      <%= urls[url] %>
      <a href=<%= "/urls/" + url + "/" %> >Edit</a>
      <form style="display:inline-block" method="POST" action=
        <%="/urls/"  + url + "?_method=DELETE"%>>
        <input type="submit" value="Delete">
      </form>
    </li>
  <% } %>
  </ul>
</body>
</html>